{
  "name": "starter-next-tailwind",
  "version": "0.1.0",
  "description": "Starter template with next and tailwindcss",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "prettier": "prettier --write '*/**/*.{js,ts,tsx,json,md}'",
    "lint:eslint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "lint:stylelint": "stylelint '**/*.{css,tsx}'",
    "lint": "yarn lint:eslint && yarn lint:stylelint"
  },
  "prettier": "@mullerstd/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@mullerstd/eslint-config"
  },
  "stylelint": {
    "extends": [
      "@mullerstd/stylelint-config"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@yarnpkg/pnpify": "^3.0.0-rc.6",
    "next": "^10.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^12.1.4",
    "@mullerstd/eslint-config": "^1.3.0",
    "@mullerstd/stylelint-config": "^1.0.8",
    "@mullerstd/tsconfig": "^1.0.0",
    "@types/eslint": "^7.2.13",
    "@types/node": "^15.12.1",
    "@types/prettier": "^2.2.3",
    "@types/react": "^17.0.9",
    "@types/tailwindcss": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "autoprefixer": "latest",
    "commitlint": "^12.1.1",
    "eslint": "^7.28.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "postcss": "^8.3.0",
    "prettier": "^2.3.1",
    "stylelint": "^13.13.1",
    "tailwindcss": "^2.1.4",
    "typescript": "^4.2.4"
  }
}
